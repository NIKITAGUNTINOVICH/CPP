#include <iostream>
#include <fstream>
#include <clocale>

int main() {
    // Устанавливаем локаль для поддержки русского языка
    setlocale(LC_ALL, "Russian");

    int A, B;

    std::cout << "Введите два целых числа A и B (B > A): ";
    std::cin >> A >> B;

    // Проверка условия B > A
    if (B <= A) {
        std::cout << "Ошибка: B должно быть больше A.\n";
        return 1;
    }

    // Открываем файл для записи
    std::ofstream file("output.txt");
    if (!file.is_open()) {
        std::cout << "Ошибка: не удалось открыть файл 'output.txt' для записи.\n";
        return 1;
    }

    // Цикл от A до B включительно
    for (int i = A; i <= B; ++i) {
        file << i * 3 << "\n";
    }

    file.close();
    std::cout << "Результат записан в файл 'output.txt'.\n";

    return 0;
}
